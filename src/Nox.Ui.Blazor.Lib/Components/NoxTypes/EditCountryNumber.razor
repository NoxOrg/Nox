@namespace Nox.Ui.Blazor.Lib.Components.NoxTypes
@using MudBlazor

<MudSelect T="ushort?"
           Label="@Title"
           Variant="Variant.Outlined"
           FullWidth="true"
           Clearable="true"
           Required="@Required"
           RequiredError="@ErrorRequiredMessage"
           TextChanged="OnCountryNumberChanged"
           @bind-Value="CountryNumber">
    @foreach (var countryNumber in CountryNumbers)
    {
        <MudSelectItem T="ushort?" Value="(ushort?)countryNumber" />
    }
</MudSelect>